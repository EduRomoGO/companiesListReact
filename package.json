    "enzyme": "^2.6.0",
    "gulp": "^3.9.0",
    "gulp-jshint": "^2.0.4",
    "jasmine": "^2.5.2",
    "jsdom": "^9.8.3",
    "jshint": "^2.9.4",
    "jshint-stylish": "^2.1.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.9.1",
